{
  "apps" : [{
    "name"               : "vue-mis",
    "script"             : "index.js",
    "exec_interpreter"   : "node",
    "instances"          : 4,
    "log_date_format"    : "YYYY-MM-DD HH:mm:ss.SSS",
    "max_memory_restart" : "1400M",
    "merge_logs"         : true,
    "watch"              : false,
    "env": {
        NODE_ENV         : "production"
    },
    "error_file"         : "/var/logs/vue-mis/stderr.log",
    "out_file"           : "/var/logs/vue-mis/stdout.log",
    "pid_file"           : "/var/logs/vue-mis/vue-mis.pid"
  }]
}
